<%= form_with url: "/users", method: :post, local: :true do |f| %>
<%= f.label :name, "Name", required: true %>
<%= f.text_field :name %>
<%= f.label :email, "Email", required: true%>
<%= f.text_field :email %>
<%= f.label  :password, "Password" , required: true%>
<%= f.password_field :password %>
<%= f.label :password_confirmation, "Confirm Password", required: true %>
<%= f.password_field :password_confirmation %>
<%= f.submit "Submit" %>
<% end %>
